# Client Environment Variables
# Copy this file to .env and fill in your values

# API Base URL (where your server is running)
VITE_API_BASE_URL=http://localhost:3000

# Environment
VITE_NODE_ENV=development

# Application Info
VITE_APP_NAME=LeadConnect
VITE_APP_VERSION=1.0.0

# AWS CloudFront (for production deployment)
VITE_CLOUDFRONT_DOMAIN=

# Stripe (if using Stripe for payments)
VITE_STRIPE_PUBLISHABLE_KEY=

# Analytics (if using analytics)
VITE_GOOGLE_ANALYTICS_ID=

# Feature Flags
VITE_ENABLE_2FA=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_IMPORT=true

# Debug Mode
VITE_DEBUG=false
